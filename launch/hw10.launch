<launch>
    <env
        name="ROSCONSOLE_CONFIG_FILE"
        value="$(find ros-navigator)/config/config.conf"
    />
    <arg name = "frequency" default = "10" />
    <node
        pkg = "ros-navigator"
        type = "talker"
        name = "talker"
        output = "screen"
        args="$(arg frequency)"
    />
    
    <node
        pkg = "ros-navigator"
        type = "listener"
        name = "listener"
        output = "screen"
        launch-prefix = "gnome-terminal -e"
    />
</launch>
